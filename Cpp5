#include <iostream>
using namespace std;
int multiply(int a, int b = 2, int c = 3) 
{
    return a * b * c;
}
int main() 
{
    cout << "Calling multiply(5): " << multiply(5) << endl;           
    cout << "Calling multiply(5, 4): " << multiply(5, 4) << endl;      
    cout << "Calling multiply(5, 4, 2): " << multiply(5, 4, 2) << endl;  
    return 0;
}
