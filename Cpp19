#include <iostream>
using namespace std;
int value = 10; 
namespace Demo 
{
    int value = 20; 
}
int main() 
{
    int value = 30; 
    cout << "Local value: " << value << endl;
    cout << "Namespace value: " << Demo::value << endl; 
    cout << "Global value: " << ::value << endl;
    return 0;
}
