#include <iostream>
using namespace std;
class Rectangle 
{
private:
    int length, width;

public:
    Rectangle() 
    {
        length = 0;
        width = 0;
        cout << "Default Constructor called." << endl;
    }
    Rectangle(int l) 
    {
        length = l;
        width = l;  
        cout << "Constructor with one parameter called." << endl;
    }
    Rectangle(int l, int w) {
        length = l;
        width = w;
        cout << "Constructor with two parameters called." << endl;
    }
    int area() 
    {
        return length * width;
    }
};

int main() 
{
    Rectangle r1;
    cout << "Area of r1: " << r1.area() << endl << endl;
    Rectangle r2(5);
    cout << "Area of r2: " << r2.area() << endl << endl;
    Rectangle r3(4, 6);
    cout << "Area of r3: " << r3.area() << endl << endl;
    return 0;
}

